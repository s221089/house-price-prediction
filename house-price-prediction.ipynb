    "# log変換\n",
    "y_log = np.log1p(y_train)\n",
    "\n",
    "# reset_indexしてインデックス揃える\n",
    "X_train = X_train.reset_index(drop=True)\n",
    "y_log = y_log.reset_index(drop=True)\n",
